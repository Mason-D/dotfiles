!! st settings
! Terminal Name
st.termname: st-256color

! Shell
st.shell: /bin/zsh

! Font
st.font: Hack Nerd Font:size=12

! Coloring
st.background: black
st.foreground: white

! Transparency (0-255):
st.alpha: 180

